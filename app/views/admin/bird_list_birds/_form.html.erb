<%= simple_form_for([:admin, @bird_list_bird]) do |f| %>
  <input type="hidden" name="page" value="<%= @page %>">
  <%= f.error_notification %>
  <%= f.association :rarity, required: true %>
  <%= f.input :resident %>
  <%= f.input :summer_visitor %>
  <%= f.input :winter_visitor %>
  <%= f.input :migrant %>
  <%= f.input :vagrant %>

  <%= f.button :submit %>
<% end %>
