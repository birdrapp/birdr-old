<div class="page-header">
  <div class="container">
    <h5 class="mb-0"><%= @club_membership %></h5>
  </div>
</div>

<div class="container">
  <div class="row">
    <div class="col-12">
      <dl class="row">
        <dt class="col-sm-3">User</dt>
        <dd class="col-sm-9"><%= link_to @club_membership.user, admin_user_path(@club_membership.user) %></dd>
      </dl>
      <dl class="row">
        <dt class="col-sm-3">Club</dt>
        <dd class="col-sm-9"><%= link_to @club_membership.club, admin_club_path(@club_membership.club) %></dd>
      </dl>
      <dl class="row">
        <dt class="col-sm-3">Status</dt>
        <dd class="col-sm-9"><%= @club_membership.status %></dd>
      </dl>
    </div>
  </div>
  <div class="row">
    <div class="col-12">
      <%= link_to 'Edit', edit_admin_club_membership_path(@club_membership), class: 'btn btn-outline-dark' %>
      <%= link_to 'Destroy', admin_club_membership_path(@club_membership), method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger' %>
    </div>
  </div>
</div>



